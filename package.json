{
  "name": "tonejs-json-sequencer",
  "version": "1.0.0",
  "description": "A lightweight library that allows you to define Tone.js instruments, effects, and performances using JSON",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "NPM_README.md"
  ],
  "readme": "NPM_README.md",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm && node scripts/rename-to-mjs.js",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && node scripts/copy-to-dist.js"
  },
  "keywords": [
    "tonejs",
    "tone.js",
    "sequencer",
    "json",
    "music",
    "audio",
    "web-audio"
  ],
  "author": "cat2151",
  "license": "MIT",
  "peerDependencies": {
    "tone": "^15.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tone": "^15.0.4",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cat2151/tonejs-json-sequencer.git"
  }
}
